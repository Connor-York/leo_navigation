<?xml version="1.0"?>
<launch>

  <arg name="map_file" default="$(find leo_navigation)/maps/deskspace_large.yaml"/>

  <node name="map_server"
    pkg="map_server"
    type="map_server"
    args="$(arg map_file)" />

  <node name="gmapping"
    pkg="gmapping"
    type="slam_gmapping"
    clear_params="true">
    <rosparam command="load"
      file="$(find leo_navigation)/config/slam_gmapping.yaml" />
  </node>
</launch>
