<?xml version="1.0"?>
<launch>
  <arg name="map_file" default="$(find leo_navigation)/maps/BIGMAP_edited.yaml"/>

  <node name="map_server"
    pkg="map_server"
    type="map_server"
    args="$(arg map_file)" />

  <node name="amcl"
    pkg="amcl"
    type="amcl"
    clear_params="true">
    <rosparam command="load" file="$(find leo_navigation)/config/amcl_2.yaml" />
    <remap from="scan" to="scan_filtered"/>
  </node>
</launch>
